{
  "name": "hangman",
  "version": "1.0.0",
  "description": "",
  "keywords": [],
  "author": "Zhenyu Lin",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git://github.com/zhenyulin/hangman.git"
  },
  "scripts": {
    "start": "webpack --config config/webpack.server.dev.js --progress",
    "watch": "esw . --watch --changed --fix",
    "lint": "eslint . --fix",
    "stylelint": "stylelint client/**/*.js",
    "test": "jest",
    "cover": "jest --coverage",
    "e2e:setup": "node_modules/selenium-standalone/bin/selenium-standalone install",
    "e2e": "node_modules/.bin/nightwatch --config config/nightwatch.js",
    "build:client": "webpack -p --config config/webpack.client.prod.js --progress --bail",
    "build:server": "webpack -p --config config/webpack.server.prod.js --progress --bail",
    "build": "npm run build:server && npm run build:client",
    "serve": "NODE_ENV=production node dist/server.js",
    "serve:fresh": "npm run build && npm run serve"
  },
  "jest": {
    "modulePaths": [
      "client",
      "server"
    ],
    "snapshotSerializers": [
      "<rootDir>/node_modules/enzyme-to-json/serializer"
    ]
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.5.2",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.16.0",
    "babel-register": "^6.24.1",
    "coveralls": "^2.13.1",
    "cucumber": "^2.0.0-rc.9",
    "enzyme": "^2.6.0",
    "enzyme-to-json": "^1.5.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-import-resolver-webpack": "^0.8.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jest": "^20.0.3",
    "eslint-plugin-json": "^1.2.0",
    "eslint-plugin-jsx-a11y": "^5.0.1",
    "eslint-plugin-react": "^7.0.1",
    "eslint-watch": "^3.1.0",
    "file-loader": "^0.10.1",
    "html-loader": "^0.4.5",
    "jest": "^20.0.4",
    "morgan": "^1.8.1",
    "nightwatch": "^0.9.9",
    "react-addons-perf": "^15.3.2",
    "react-addons-test-utils": "^15.6.0",
    "redux-devtools-extension": "^2.13.2",
    "selenium-standalone": "^5.8.0",
    "selenium-webdriver": "3.4.0",
    "start-server-webpack-plugin": "^2.2.0",
    "stylelint": "^7.10.1",
    "stylelint-config-standard": "^16.0.0",
    "stylelint-processor-styled-components": "^0.1.0",
    "webpack": "^3.0.0",
    "webpack-dev-middleware-webpack-2": "^1.5.1",
    "webpack-hot-middleware": "^2.18.0",
    "webpack-node-externals": "^1.6.0"
  },
  "dependencies": {
    "body-parser": "^1.17.1",
    "compression": "^1.6.2",
    "cors": "^2.8.3",
    "express": "^4.14.0",
    "express-validator": "^3.2.0",
    "favicon": "^0.0.2",
    "helmet": "^3.5.0",
    "immutable": "^3.8.1",
    "jsonwebtoken": "^7.3.0",
    "mongoose": "^4.9.2",
    "passport": "^0.3.2",
    "passport-jwt": "^2.2.1",
    "passport-local": "^1.0.0",
    "prop-types": "^15.5.10",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-redux": "^5.0.5",
    "react-router": "^4.1.1",
    "react-router-redux": "^5.0.0-alpha.6",
    "redux": "^3.6.0",
    "redux-segment": "^1.6.1",
    "reselect": "^3.0.0",
    "socket.io": "^2.0.3",
    "socket.io-client": "^2.0.3",
    "styled-components": "^2.0.0",
    "styled-components-theme": "^1.0.5"
  }
}
