  "engines": {
    "node": "^6.10.0",
    "npm": "^5.0.0",
    "yarn": "^0.24.6"
  },
  "scripts": {
    "start": "nodemon --config config/nodemon.json server/server.js",
    "watch": "npm-watch",
    "lint": "eslint server client config test --fix",
    "lint:watch": "esw server client config test --watch --changed --clear --fix",
    "stylelint": "stylelint client/**/*.js",
    "test": "jest server client",
    "coverage": "npm run test -- --coverage",
    "e2e:setup": "node_modules/selenium-standalone/bin/selenium-standalone install",
    "e2e": "node_modules/.bin/nightwatch --config config/nightwatch.js",
    "build:server": "babel server -d dist/server --copy-files --ignore server/**/*.spec.js",
    "build:client": "webpack -p --config config/webpack.prod.js --progress --bail",
    "build": "npm run build:server && npm run build:client",
    "serve": "NODE_PATH=./dist NODE_ENV=production node dist/server/server.js",
    "serve:fresh": "npm run build && npm run serve"
  },
  "watch": {
    "lint": "server client config test",
    "stylelint": "client/**/*.js",
    "test": "client server"
  },
